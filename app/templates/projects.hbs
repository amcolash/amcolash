<h1>Projects</h1>
<hr/>

<h2>My Github Projects <img src="img/GitHub-Mark-32px.png"/></h2>

{{#each}}
  <div class="well">
    <h3><a target="_blank" {{bindAttr href=html_url}}>{{name}}</a></h3>
    <hr/>
    <div class="row">
      <div class="col-md-8">
        <p>{{description}}</p>
      </div>
      <div class="col-md-4">
        <p>Last Push: {{pretty-date pushed_at}}</p>
        <p>Created: {{pretty-date created_at}}</p>
      </div>
    </div>
  </div>
{{/each}}