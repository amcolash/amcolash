<h1>Welcome to Andrew's Website - Projects</h1>
<hr/>

<h2>My Github Projects</h2>

{{#each}}
  <div class="well">
    <h3><a target="_blank" {{bindAttr href=html_url}}>{{name}}</a></h3>
    <hr/>
    <div class="row">
      <div class="col-xs-8">
        <p>{{description}}</p>
      </div>
      <div class="col-xs-4">
        <p>Last Push: {{pretty-date pushed_at}}</p>
        <p>Created: {{pretty-date created_at}}</p>
      </div>
    </div>
  </div>
{{/each}}

<h2>Other Projects</h2>